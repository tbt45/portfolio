e}}</div>\n            </div>\n        <!-- </div> -->\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n    data: function() {\n        return {\n            name: \"\",\n            comment: \"\",\n            posts: []\n        };\n    },\n    created() {\n        axios.get(\n            'https://firestore.googleapis.com/v1/projects/test-74b65/databases/(default)/documents/comments'\n        ).then(response => {\n            this.posts = response.data.documents;\n        });\n    },\n    methods: {\n        Home() {\n            this.$router.push(\"/\");\n        },\n        createComment(){\n            axios.post('https://firestore.googleapis.com/v1/projects/test-74b65/databases/(default)/documents/comments',\n            {\n                return(){\n                    alert(alert)\n                },\n                fields: {\n                    name: {\n                        stringValue: this.name\n                    },\n                    comment: {\n                        stringValue: this.comment\n                    }\n                }\n            }\n            )\n        this.name = \"\";\n        this.comment = \"\";\n        },\n        remove(){\n            this.post.shift()\n        }\n    },\n    computed: {\n        image_src(){return this.$store.state.image.url}\n    },\n};\n</script>\n\n<style>\n    .name{\n        display: block;\n    }\n    .comment{\n        color: crimson;\n        border: solid red;\n    }\n    .main{\n        background-image: url(\"/src/images/mac.jpeg\");\n        height: 500px;\n        width: 500px;\n    }\n</style>"]}]}